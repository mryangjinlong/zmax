        <!--    ---------------------------------------------------------------------------------------   -->
        <!--    选座    -->
        <div class="page" id="page-systemlogin">
            <div class="seatTop bgNo">
                <h1>登录</h1>
                <a class="toBack" href="index.html"></a>
            </div>
            <div class="systemloginBody">
                <div class="sysLoginBox">
                    <input class="count" type="text" placeholder="请输入账号">
                    <input class="psw" type="password" placeholder="请输入密码">
                    <div class="numKeyBoard allkeyboxposition">
                        <ul>
                            <li class="key">1</li>
                            <li class="key">2</li>
                            <li class="key">3</li>
                        </ul>
                        <ul>
                            <li class="key">4</li>
                            <li class="key">5</li>
                            <li class="key">6</li>
                        </ul>
                        <ul>
                            <li class="key">7</li>
                            <li class="key">8</li>
                            <li class="key">9</li>
                        </ul>
                        <ul>
                            <li class="reinput">
                                清空
                            </li>
                            <li class="key">0</li>
                            <li class="key deleteBack">
                                <img src="./img/login/delete-icon.png" alt="">
                            </li>
                        </ul>
                    </div>
                    <div class="toLogin">登录</div>
                </div>
            </div>
        </div>
        <!--    选座结束    -->
