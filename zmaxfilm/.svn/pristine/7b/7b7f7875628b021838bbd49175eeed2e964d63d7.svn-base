/*样式重置*/

body {
    -moz-user-select: none;
    /*火狐*/
    -webkit-user-select: none;
    /*webkit浏览器*/
    font-size: 0.24rem;
    font-family: "Microsoft YaHei", 微软雅黑;
}

@font-face {
    font-family: dr;
    src: url(../font/DS-DIGIB.TTF);
}

input,
button {
    font-family: Arial;
    border: medium none;
}

button {
    background: none;
    border: none;
}

input,
select,
button {
    outline: medium none;
}

textarea {
    font-family: inherit;
    font-size: inherit;
    font-weight: inherit;
    resize: none;
    outline: none;
}

em,
i {
    font-style: normal;
}

div {
    margin: 0 auto;
    padding: 0;
}

h1,
h2,
h3,
h4,
h5,
h6,
ul,
li,
dl,
dt,
dd,
form,
img,
p {
    margin: 0;
    padding: 0;
    border: none;
    list-style-type: none;
}

h1,
h2,
h3,
h4,
h5,
h6 {
    font-weight: normal;
}

a {
    text-decoration: none;
    color: #FFF;
}

table {
    border-collapse: collapse;
}


/*样式重置结束*/

.wrap {
    width: 10.8rem;
    height: 19.2rem;
    overflow: hidden;
    position: relative;
}


/*定义滚动条高宽及背景 高宽分别对应横竖滚动条的尺寸*/
::-webkit-scrollbar
{
    width: .16rem;
    height: .16rem;
    background-color: #2E2F32;
}
/*定义滚动条轨道 内阴影+圆角*/
::-webkit-scrollbar-track
{
    -webkit-box-shadow: inset 0 0 0.06rem rgba(0,0,0,0.3);
    border-radius: 0.1rem;
    background-color: #707378;
}
 
/*定义滑块 内阴影+圆角*/
::-webkit-scrollbar-thumb
{
    border-radius: 10px;
    -webkit-box-shadow: inset 0 0 0.06rem rgba(0,0,0,.3);
    background-color: #fff;
}

/*头部跟底部*/

.disno {
    display: none !important;
}

#header {
    width: 100%;
    height: 6.075rem;
    z-index: 9999;
}

#header img {
    width: 100%;
    height: 100%;
}

.swiper-slide {
    text-align: center;
    background: #fff;
    /* Center slide text vertically */
    display: -webkit-box;
    display: -ms-flexbox;
    display: -webkit-flex;
    display: flex;
    -webkit-box-pack: center;
    -ms-flex-pack: center;
    -webkit-justify-content: center;
    justify-content: center;
    -webkit-box-align: center;
    -ms-flex-align: center;
    -webkit-align-items: center;
    align-items: center;
}

#footer {
    position: fixed;
    bottom: 0rem;
    left: 0rem;
    width: 100%;
    height: 0.605rem;
    line-height: 0.605rem;
    text-align: center;
    color: #fff;
    background: #272527;
    font-size: 0.24rem;
    z-index: 9999;
}


/*头部跟底部*/


/*首页*/

