            var speed = speedNum;
            timer = setInterval(function () {
                var _this_top = parseFloat($('.pAdminit').css('top'));
                _this_top += speed;

                if (_this_top >= 197) {
                    clearInterval(timer);
                    setTimeout(function () {
                        $('.pAdminit').fadeOut("slow", function () {
                            $('.pAdminit').css('top', '-3.45rem');
                            clearTimeout(_timer);
                            var _timer;
                            _timer = setTimeout(function () {
                                $('.pAdminit').show();
                                _showImg(10);
                            }, 1000)
                        })
                    }, 500)
                } else {
                    $('.pAdminit').css('top', (_this_top / 100) + 'rem');
                }
            }, 50)
        }
        _showImg(10)
